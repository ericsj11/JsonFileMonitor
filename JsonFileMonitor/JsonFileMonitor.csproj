  <ItemGroup>
    <ProjectReference Include="..\JsonFileMonitor.Abstractions\JsonFileMonitor.Abstractions.csproj" />
    <ProjectReference Include="..\JsonFileMonitor.Data\JsonFileMonitor.Data.csproj" />
  </ItemGroup>

</Project>
